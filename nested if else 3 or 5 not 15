#include <stdio.h>

int main()
{  int n;
    printf("Enter the no.:");
     scanf("%d",&n);
    if(n%5==0 || n%3==0){
     if(n%15!=0){
      printf("The no. is divisible by 5 and 3 but not 15");
     }
     else{
     printf("The no. is  divisible 15");
     }
    }
    else{
     printf("The no. is not divisible by 5 and 3");
     }
    return 0;
}
