#include <stdio.h>
int power (int a, int b){
   int s=1;
   for(int i=1;i<=b;i++){
     s=s*a;
   }
   return s;
}
int main()
{ int a;
    printf("Enter the no. a:");
    scanf("%d",&a);
    int b;
    printf("Enter the no. b:");
    scanf("%d",&b);
    int p=power(a,b);
    printf("%d raised to the power %d is %d:",a,b,p);
    return 0;
}