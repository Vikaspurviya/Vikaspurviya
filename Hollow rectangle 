#include <stdio.h>

int main()
{ int r,c;
    printf("Enter the no. of rows :");
    scanf("%d",&r);
    printf("Enter the no. of column:");
    scanf("%d",&c);
    for(int i=1;i<=r;i++){
    for(int j=1;j<=c;j++){
    if(i==1 || i==r || j==1 || j==c){
      printf("*");}
      else{ printf(" ");
       } 
    }
       printf("\n");
}

    return 0;
}