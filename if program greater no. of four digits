#include <stdio.h>

int main()
{ int a,b,c,d;
    printf("Enter frist no. :");
    scanf("%d",&a);
    printf("Enter second no.:");
    scanf("%d",&b);
    printf("Enter third no.:");
    scanf("%d",&c);
    printf("Enter fourth no.:");
    scanf("%d",&d);
    if(a>b && a>c && a>d){
    printf("is greater no. %d :",a);
    }
    if(b>a && b>c && b>d){
    printf(" is greater no. %d :",b);
    }
    if(c>a && c>b && c>d) {
    printf("is greater no. %d :",c);
    }
    if(d>a && d>b && d>c) {
    printf(" is greater no. %d :",d);
    }
    return 0;
}