#include <stdio.h>

int main()
{ int arry[8]={1,2,3,4,5,6,7,8};
int totaltriplets=0;
int x=12;
  for(int i=0;i<=7;i++){
      for(int j=i+1;j<=7;j++){
       for(int k=j+1;k<=7;k++){
    if(arry[i]+arry[j]+arry[k]==x) {
       totaltriplets++;
       printf("(%d,%d,%d)",arry[i],arry[j],arry[k]);
    }
} 
      }
  }
    printf("\nTotal pairs:%d",totaltriplets);

    return 0;
}