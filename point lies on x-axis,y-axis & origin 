#include <stdio.h>

int main()
{  int x,y;
    printf("Enter the coordinates :");
    scanf("%d%d",&x,&y);
    if(y==0 && x==0){
    printf("The point lies on origin");
    }
    else if(x==0){
    printf("lies on y-axis");
    }
    else if(y==0){
    printf("lies on x-axis ");
    } 
    else{
    printf("the point does not lies on x-axis and y-axis");
    }
    return 0;
}