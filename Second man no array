#include <stdio.h>
#include<limits.h>
int main()
{ int arry[8] = {1,2,3,4,5,6,7,8};
  int max=INT_MIN;
  int smax=INT_MIN;
  for(int i=0;i<=7;i++){
     if(max<arry[i]){
         smax=max;
         max=arry[i];
     }
     else if(smax<arry[i]){
       smax=arry[i];
     }
  }
     printf("The second largest no. is :%d",smax);



    return 0;
}